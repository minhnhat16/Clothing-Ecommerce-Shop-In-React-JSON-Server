{
  "name": "my-ecommerce-app",
  "private": true,
  "version": "0.0.0",
  "homepage": "https://minhnhat16.github.io/my-ecommerce-app",
  "type": "module",
  "scripts": {
    "deploy": "gh-pages -d dist",
    "start": "concurrently \"npm run dev\" \"npm run json\"",
    "dev": "vite",
    "build": "vite build",
    "test": "jest",
    "lint": "eslint src --ext js,jsx,ts,tsx,css,html --report-unused-disable-directives --max-warnings 0",
    "lint:fix ": "eslint src --ext js,jsx,ts,tsx,css,html --fix",
    "format": "prettier --write 'src/**/*.{js,jsx,ts,tsx,css,html}'",
    "preview": "vite preview",
    "json": "json-server --watch src/data/db.json --port 8080",
    "gh-pages": "gh-pages -d dist"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^1.9.7",
    "axios": "^1.6.0",
    "concurrently": "^8.2.2",
    "html-react-parser": "^5.0.3",
    "nanoid": "^5.0.2",
    "react": "^18.2.0",
    "react-auth-kit": "^3.0.0-alpha.35",
    "react-datepicker": "^4.21.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.11.0",
    "react-redux": "^8.1.3",
    "react-router-dom": "^6.17.0",
    "react-toastify": "^9.1.3"
  },
  "devDependencies": {
    "@babel/core": "^7.24.7",
    "@babel/preset-env": "^7.24.7",
    "@babel/preset-react": "^7.24.7",
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@tailwindcss/typography": "^0.5.10",
    "@testing-library/dom": "^10.1.0",
    "@testing-library/jest-dom": "^6.4.6",
    "@testing-library/react": "^16.0.0",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@vitejs/plugin-react": "^4.0.3",
    "autoprefixer": "^10.4.16",
    "babel-jest": "^29.7.0",
    "daisyui": "^3.9.4",
    "esbuild": "^0.21.5",
    "esbuild-jest": "^0.4.0",
    "eslint": "^8.45.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.34.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "gh-pages": "^6.1.1",
    "husky": "^9.0.11",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-transform-stub": "^2.0.0",
    "json-server": "^0.17.4",
    "lint-staged": "^15.2.7",
    "postcss": "^8.4.31",
    "prettier": "^3.3.2",
    "react-test-renderer": "^18.3.1",
    "tailwindcss": "^3.3.5",
    "vite": "^4.4.5"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm run lint && npm run format"
    }
  },
  "lint-staged": {
    "src/**/*.{ts,tsx,js,jsx}": [
      "npm run lint",
      "npm run format"
    ]
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ]
  }
}
